<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">

        <title>{% block title %}Welcome!{% endblock %}</title>
        
        <meta content="" name="description">
        
        <meta content="" name="keywords">
        
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- Google Fonts -->
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
        <!-- Favicons -->
        <link href="{{ asset('assets/img/logo/pees_transparent_2.png') }}" rel="icon">
        <link href="{{ asset('assets/img/logo/pees_transparent_2.png') }}" rel="apple-touch-icon">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <!-- Vendor CSS Files -->
            <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
            <link rel="stylesheet" href="assets/css/bootstrap-5.0.0-alpha-2.min.css" />
            <link rel="stylesheet" href="assets/css/LineIcons.2.0.css"/>
            <link rel="stylesheet" href="assets/css/tiny-slider.css"/>
            <link rel="stylesheet" href="assets/css/glightbox.min.css"/>
            <link rel="stylesheet" href="assets/css/animate.css"/>
            <link rel="stylesheet" href="assets/css/lindy-uikit.css"/>

        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
            <script src="assets/js/bootstrap.5.0.0.alpha-2-min.js"></script>
            <script src="assets/js/tiny-slider.js"></script>
            <script src="assets/js/count-up.min.js"></script>
            <script src="assets/js/imagesloaded.min.js"></script>
            <script src="assets/js/isotope.min.js"></script>
            <script src="assets/js/glightbox.min.js"></script>
            <script src="assets/js/wow.min.js"></script>
            <script src="assets/js/main.js"></script>
            {% endblock %}
    </head>
    <body>
        <div class="container">
        </div>
        <div class="preloader">
            <div class="loader">
              <div class="spinner">
                <div class="spinner-container">
                  <div class="spinner-rotator">
                    <div class="spinner-left">
                      <div class="spinner-circle"></div>
                    </div>
                    <div class="spinner-right">
                      <div class="spinner-circle"></div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
        </div>
        {#{{ include('shared/_menu.html.twig') }}#}
        {% block body %}{% endblock %}
        {{ include('shared/_footer.html.twig') }}
    </body>
</html>
